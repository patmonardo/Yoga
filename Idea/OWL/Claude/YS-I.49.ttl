            :ProjectiveGround
            :FacticalBasis
        )
    ] .

# Properties

:reveals a owl:ObjectProperty ;
    rdfs:label "reveals"@en ;
    rdfs:domain :SelfDeconstructionPattern ;
    rdfs:range :RelationalTrap .

:necessitates a owl:ObjectProperty ;
    rdfs:label "necessitates"@en ;
    rdfs:domain :SelfDeconstructionPattern ;
    rdfs:range :SystematicAbandonment .

:manifests a owl:ObjectProperty ;
    rdfs:label "manifests"@en ;
    rdfs:domain :CircularDependence ;
    rdfs:range :MutualPresupposition .

# Validation Rules

:DeconstructionValidation a owl:Class ;
    rdfs:label "Deconstruction Validation"@en ;
    :requires [
        a owl:Restriction ;
        owl:onProperty :reveals ;
        owl:someValuesFrom :RelationalTrap
    ] ;
    :validates [
        a owl:Restriction ;
        owl:onProperty :necessitates ;
        owl:someValuesFrom :SystematicAbandonment
    ] .

# Systematic Implications

:PhilosophicalImplications a owl:Class ;
    rdfs:label "Philosophical Implications"@en ;
    :entails [
