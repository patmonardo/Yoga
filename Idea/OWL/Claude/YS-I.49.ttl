@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Core Self-Deconstruction Structure

:SelfDeconstructionPattern a owl:Class ;
    rdfs:label "Self-Deconstruction Pattern"@en ;
    rdfs:subClassOf :TranscendentalPattern ;
    :reveals :RelationalTrap ;
    :exposes :HiddenGround ;
    :necessitates :SystematicAbandonment .

# Relational Trap

:RelationalTrap a owl:Class ;
    rdfs:label "Relational Trap"@en ;
    :hasElements [
        a owl:Class ;
        owl:intersectionOf (
            :EssentialQualification
            :RequiresOpposition
            :InherentRelativity
        )
    ] .

:CircularDependence a owl:Class ;
    rdfs:label "Circular Dependence"@en ;
    :manifests [
        a owl:Class ;
        owl:intersectionOf (
            :UnityThroughDuality
            :DualityThroughUnity
            :MutualPresupposition
        )
    ] .

# Hidden Structure

:BackgroundProjection a owl:Class ;
    rdfs:label "Background Projection"@en ;
    :contains [
        a owl:Class ;
        owl:intersectionOf (
            :InItselfAndNotInItself
            :ReciprocalPositing
            :MutualNegation
        )
    ] .

:ProjectionPattern a owl:Class ;
    rdfs:label "Projection Pattern"@en ;
    :characterized_by [
        a owl:Class ;
        owl:intersectionOf (
