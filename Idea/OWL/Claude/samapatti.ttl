        sp:text """
            SELECT ?principle ?manifestation
            WHERE {
                ?principle a :PrincipleOfKnowing .
                ?principle :hasManifestion ?manifestation .
                FILTER NOT EXISTS {
                    ?manifestation a :Conceptual .
                }
            }
        """
    ] .

